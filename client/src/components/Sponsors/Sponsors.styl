.sponsors-container
  background: white
  h2
    padding: 50px 0
  .sponsors
    width: 100%
    display: flex
    justify-content: space-around
    align-items: center
    @media all and (max-width: 1200px)
      flex-direction: column
      justify-content: center
    img
      width: 250px
      height: 250px
      object-fit: scale-down
      margin: 50px 0
